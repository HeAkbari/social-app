{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "web-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/web-app",
      "sourceRoot": "projects/web-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser-esbuild",
          "options": {
            "outputPath": "dist/web-app",
            "index": "projects/web-app/src/index.html",
            "main": "projects/web-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/web-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/web-app/src/favicon.ico",
              "projects/web-app/src/assets",
              "projects/web-app/src/manifest.json"
            ],
            "styles": [
              "projects/web-app/src/assets/css/styles.scss",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "projects/web-app/src/assets/js/bootstrap.bundle.min.js"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "projects/web-app/ngsw-config.json"
          },
          "configurations": {
            "development": {
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web-app/src/environments/environment.ts",
                  "with": "projects/web-app/src/environments/environment.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web-app/src/environments/environment.ts",
                  "with": "projects/web-app/src/environments/environment.prod.ts"
                }
              ]
            },
            "staging": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/web-app/src/environments/environment.ts",
                  "with": "projects/web-app/src/environments/environment.staging.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "web-app:build:production"
            },
            "development": {
              "browserTarget": "web-app:build:development"
            },
            "staging": {
              "browserTarget": "web-app:build:staging"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/web-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/web-app/src/favicon.ico",
              "projects/web-app/src/assets",
              "projects/web-app/src/manifest.json"
            ],
            "styles": [
              "projects/web-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/web-app/.storybook",
            "browserTarget": "web-app:build",
            "compodoc": false,
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/web-app/.storybook",
            "browserTarget": "web-app:build",
            "compodoc": false,
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "fbf6290f-f919-4ed5-b8b8-2bc86ab6948e"
  }
}